# Production environment variables
# These are loaded by Vite during production builds

# Disable debug mode in production
VITE_DEBUG=false

# Maximum polygons to render per frame (prevents OOM)
VITE_MAX_POLYGONS_PER_RENDER=100000

# FPS counter update interval in milliseconds
VITE_FPS_UPDATE_INTERVAL=500

# Signaling Server Configuration (for P2P collaboration)
# These should be set in GitHub Secrets for production deployment
# VITE_SIGNALING_SERVER_URL=wss://signaling.gdsjam.com
# VITE_SIGNALING_SERVER_TOKEN=<set-in-github-secrets>

# TURN Server Configuration (for WebRTC NAT traversal)
# VITE_TURN_PASSWORD=<set-in-github-secrets>

